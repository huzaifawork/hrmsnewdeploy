{
  "version": 2,
  "name": "hrms-backend",
  "builds": [
    {
      "src": "index.serverless.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.serverless.js"
    }
  ],
  "functions": {
    "index.serverless.js": {
      "maxDuration": 30
    }
  }
}
